  <body>
    <div class="filtr"></div>
    <div class="container"><br>
      <h2 align="center">Add Voter</h2>
      <div class="row">
        <div class="col">	
        </div>		
        <div class="col-md-5 col-sm-11">
          <br>
          <span class="message"><%= message %></span>
          <form class="form" method="POST" action=""> 
            <div class="form-group" >
            <img src="https://www.w3schools.com/howto/img_avatar.png" alt="Avatar" >
            </div>
            <span id="message1"></span>
            <div class="form-group">
              <label for="fname">First Name:</label>
              <input type="text" id="fname" name="fname" class="form-control alpha-only" placeholder="Enter First Name" required="True">
            </div>
            <div class="form-group">
              <label for="lname">Last Name:</label>
              <input type="text" id="lname" name="lname" class="form-control alpha-only" placeholder="Enter Last Name"	required="True">
            </div>
            <div class="form-group">
                <label for="lname">Enter Adhar:</label>
                <input type="text" id="lname" name="adhar" class="form-control alpha-only" placeholder="Enter Last Name"	required="True">
            </div>
      
            <div class="form-group">
              <label for="dob">Date of Birth:</label>
              <input type="Date" id="datefield" name="dob" max='2018-22-09' class="form-control" >
            </div>
            <div class="form-group">
              <label for="gender">Gender:</label>
              <select class="form-control" name="gender">
                <option>Male</option>
                <option>Female</option>
                <option>Other</option>
              </select>
            </div>
            <div class="form-group">
              <label for="exampleTextarea">Address:</label>
              <textarea class="form-control" id="exampleTextarea" rows="3" name="address"></textarea>
            </div>
  
            <!-- <div class="form-group">
              <label >Select a file for age proof: </label><br>
            <input type="file" name="myFile" required="True"><p style="color: red">AADHAAR Card, Birth Certificate etc. </p>
            </div><hr>
  
            <div class="form-group">
              <label >Select a file for address proof: </label><br>
            <input type="file" name="myFile1" required="True"><p style="color: red">AADHAAR Card, Electricity Bill etc. </p>
            </div><hr>
  
            <div class="form-group">
              <label >Upload Photo:</label><br>
            <input type="file" name="myFile2" required="True">
  
            </div> -->
            <br>
            <div class="but">
              <button type="submit"  class="btn but1">Submit Form</button>
            </div>
          </form>
        </div>
        <div class="col">	
        </div>
      </div>
    </div>
  
  
